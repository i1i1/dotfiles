    xbacklight -dec 10 && notify-send "$(printf 'Brightness %.0f%%\n' $(xbacklight))"
super + braceright
    xbacklight -inc 10 && notify-send "$(printf 'Brightness %.0f%%\n' $(xbacklight))"
